@model SecurityWaitTimeResponse
 
<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<title>@ViewData["Title"]</title>
	</head>
<body>
	<h1>@ViewData["Title"]</h1>
	<table>
		<thead>
			<tr>
				<th>Queue name</th>
				<th>Current time</th>
				<th>Current projected wait time</th>
				<th>Terminal</th>
				<th>Overflow</th>
			</tr>
		</thead>
		<tbody>
			@foreach(var waitTime in Model.WaitTimes)
			{	<tr>
				<td>@waitTime.QueueName</td>
				<td>@waitTime.CurrentTime</td>
				<td>@waitTime.CurrentProjectedWaitTime</td>
				<td>@waitTime.Terminal</td>
				<td>@waitTime.Overflow</td>
				</tr>
			}
		</tbody>
	</table>
</body>
</html>